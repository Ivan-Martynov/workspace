!! xfontsel fc-list or /usr/share/fonts/X11/misc/fonts.alias

#define font1 FiraCode-Retina
#define font2 Ubuntu Mono
#define font3 Courier 10 Pitch
#define font4 DejaVu Sans Mono

URxvt*font: xft:font1:size=10:antialias=true:minspace=false
URxvt*boldFont: xft:font1:bold:size=10:antialias=true
URxvt*italicFont: xft:font1:italic:size=10:antialias=true
URxvt*boldItalicFont: xft:font1:bold:italic:size=10:antialias=true
